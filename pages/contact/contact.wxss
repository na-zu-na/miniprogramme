/* pages/contact/contact.wxss */
.bar{
  background-color: rgb(250, 250, 250);
  padding: 5px;
  height: 40px;
  align-items: center;
  display: flex;
  box-shadow: rgb(206, 204, 204) 2px 2px 2px 2px;
  margin-bottom: 10px;
}

.bar image{
  height: 35px;
  width: 35px;
  margin: 0 5px;
  border: 2px solid white;
}

.bar text{
  font-size: 17px;
  align-items: center;
  font-weight: bold;
  color: rgb(6, 10, 56);
  margin: 0 10px;
}