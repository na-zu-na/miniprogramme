/* pages/contact/contact.wxss */
.bar{
  background-color: rgb(241, 234, 163);
  padding: 5px;
  height: 50px;
  align-items: center;
  display: flex;
  box-shadow: gray 2px 2px 2px 2px;
}

.bar image{
  height: 80rpx;
  width: 80rpx;
  margin: 0 5px;
  border: 2px solid white;
}

.bar text{
  font-size: 20px;
  align-items: center;
  font-weight: bold;
  color: rgb(6, 10, 56);
  margin: 0 10px;
}